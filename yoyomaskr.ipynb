    "from yoyomaskr import llm_find_entities\n",
    "\n",
    "with open('sample_text.txt', 'r') as file:\n",
    "    sample_text = file.read()\n",
    "result = llm_find_entities(sample_text)\n",
    "for k, v in result.items():\n",
    "    print(f'{k}: {v}')"
